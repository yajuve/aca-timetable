<odoo>
    <record id="group_form_view" model="ir.ui.view">
        <field name="name">aca_timetable.group.form</field>
        <field name="model">aca_timetable.group</field>
        <field name="arch" type="xml">

            <form string="Group">
                <header position="inside">
                    <button name="%(aca_timetable.action_group_report)d"
                        string="Print Group"
                        type="action"
                        class="btn-primary" />
                </header>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="year_level_id" />
                        <field name="parent_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
